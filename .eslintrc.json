{
	"parserOptions": {
		"ecmaVersion": 5
	},
	"extends": "eslint:recommended",
	"rules": {
		"no-console": 2,
		"no-unused-vars": 2,
		"radix": [2, "always"],
		"block-scoped-var": 2,
		"consistent-return": 2,
		"curly": 2,
		"no-return-assign": 2,
		"no-void": 2,
		"no-with": 2,
		"no-shadow": 2
	},
	"globals": {
		"d3": true,
		"ReMatcher": true
	},
	"env": {
		"node": true,
		"browser": true
	},
	"root": true
}
